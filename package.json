{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "scripts": {
    "python-dep": "pip install -r requirements.txt",
    "start": "node server.js",
    "heroku-postbuild": "cd frontend && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "frontend": "cd frontend && yarn start",
    "build-front": "cd frontend && npm run build",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn frontend\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "child_process": "^1.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "mongoose": "^5.10.5",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
